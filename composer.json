{
    "name": "rbaskam/laravel-pcloud",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rbaskam\\LaravelPCloud\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rob Askam",
            "email": "rbaskam@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload-dev": {
        "psr-4": {
            "Rbaskam\\LaravelPCloud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rbaskam\\LaravelPCloud\\Providers\\CustomPCloudServiceProvider"
            ],
            "aliases": {
                "PCloud": "JohnDoe\\BlogPackage\\Facades\\App"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "6.0",
        "phpunit/phpunit": "^9.5"
    }
}
